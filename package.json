{
  "name": "html5boilerplate",
  "version": "1.0.0",
  "description": "html5 boilerplate implement pug , sass , gulp",
  "main": "index.js",
  "directories": {
    "doc": "doc"
  },
  "scripts": {
    "start": "gulp watch",
    "serve": "browser-sync start --server --startPath \"dist\" --files \"dist/**/*.+(html|css|js)\" --browser \"google chrome\" ",
    "watch": "gulp watch",
    "build:dev": "parallelshell \"npm run serve\" \"npm run watch\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonico22/html5boilerplate.git"
  },
